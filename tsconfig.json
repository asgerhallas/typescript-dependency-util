{
    "version": "1.4.1",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "outDir": "lib",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false
    },
    "filesGlob": [
        "./**/*.ts",
        "!./example/**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./src/DependencyManager.ts",
        "./src/ExportExtractor.ts",
        "./src/UsageExtractor.ts",
        "./src/index.ts",
        "./typings/mocha/mocha.d.ts",
        "./typings/node/node.d.ts",
        "./typings/typescript/typescript.d.ts"
    ]
}